 akka {
   stdout-loglevel = DEBUG
   loglevel = DEBUG
   log-config-on-start = on
   loggers=["Akka.Logger.Serilog.SerilogLogger, Akka.Logger.Serilog"]

   actor{
     debug {  
       receive = on 
       autoreceive = on
       lifecycle = on
       event-stream = on
       unhandled = on
     }    
     /servicaActor/commander/coordinator{
       router = broadcast-pool
       nr-of-instances = 3
     } 
   }
 }